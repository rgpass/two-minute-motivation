{
  "extends": ["plugin:flowtype/recommended", "airbnb", "prettier"],
  "plugins": ["flowtype"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": "off"
  },
  "globals": {
    "cy": true
  }
}
